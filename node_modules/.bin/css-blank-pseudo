#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@angular-devkit/build-angular/node_modules/css-blank-pseudo/dist/cli.cjs" "$@"
  ret=$?
else 
  node  "$basedir/../@angular-devkit/build-angular/node_modules/css-blank-pseudo/dist/cli.cjs" "$@"
  ret=$?
fi
exit $ret
